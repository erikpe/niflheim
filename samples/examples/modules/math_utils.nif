// module demo for export and re-export concepts

export fn max_i64(a: i64, b: i64) -> i64 {
    if a >= b {
        return a;
    }
    return b;
}

export fn gcd(a: i64, b: i64) -> i64 {
    var x: i64 = a;
    var y: i64 = b;

    while y != 0 {
        var t: i64 = x % y;
        x = y;
        y = t;
    }

    return x;
}
