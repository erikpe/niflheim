import std.test;
import std.io;
import std.box;


fn test_new_box_i64() -> unit {
    var x: NewBoxI64 = NewBoxI64(-7);
    assert(x.value() == -7);
}


fn test_new_box_u64() -> unit {
    var x: NewBoxU64 = NewBoxU64(123u);
    assert(x.value() == 123u);
}


fn test_new_box_u8() -> unit {
    var x: NewBoxU8 = NewBoxU8((u8)255);
    assert(x.value() == (u8)255);
}


fn test_new_box_bool() -> unit {
    var t: NewBoxBool = NewBoxBool(true);
    var f: NewBoxBool = NewBoxBool(false);
    assert(t.value() == true);
    assert(f.value() == false);
}


fn test_new_box_double() -> unit {
    var x: NewBoxDouble = NewBoxDouble(1.5);
    assert(x.value() == 1.5);
}


fn main() -> i64 {
    var select: u64 = read_all().strip().to_u64();

    if select == 1u { test_new_box_i64(); }
    if select == 2u { test_new_box_u64(); }
    if select == 3u { test_new_box_u8(); }
    if select == 4u { test_new_box_bool(); }
    if select == 5u { test_new_box_double(); }

    return 0;
}
