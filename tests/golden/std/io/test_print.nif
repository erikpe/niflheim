import std.io;

fn test_println_i64() -> unit {
    println_i64(0);
    println_i64(1);
    println_i64(-1);
    println_i64(9223372036854775807);
    println_i64(-9223372036854775808);
}

fn test_println_u64() -> unit {
    println_u64(0u);
    println_u64(1u);
    println_u64(18446744073709551615u);
    println_u64(9223372036854775808u);
}

fn test_println_u8() -> unit {
    println_u8(0u8);
    println_u8(1u8);
    println_u8(255u8);
    println_u8(128u8);
}

fn test_println_bool() -> unit {
    println_bool(true);
    println_bool(false);
}

fn test_println_double() -> unit {
    println_double(0.0);
    println_double(1.5);
    println_double(-2.25);
    println_double(123456.125);
}

fn main() -> i64 {
    test_println_i64();
    test_println_u64();
    test_println_u8();
    test_println_bool();
    test_println_double();
    return 0;
}
