import std.newstr;

fn main() -> i64 {
    var v: NewStr = " \tHello world!\n";
    var s: NewStr = v.strip();
    if s[0] != (u8)72 {
        return 1;
    }
    if s[-1] != (u8)33 {
        return 2;
    }

    var w: NewStr = " \n\t\r";
    var empty: NewStr = w.strip();
    if empty.len() != 0u {
        return 3;
    }
    return 0;
}
