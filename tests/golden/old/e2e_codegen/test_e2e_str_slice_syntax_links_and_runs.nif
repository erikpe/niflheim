import std.newstr;

fn main() -> i64 {
    var v: NewStr = "Hello world!";
    var s1: NewStr = v[3:5];
    var s2: NewStr = v[:7];
    var s3: NewStr = v[4:];
    var s4: NewStr = v[:];
    var c1: u8 = s1[0];
    var c2: u8 = s2[6];
    var c3: u8 = s3[0];
    var c4: u8 = s4[11];
    return (i64)c1 + (i64)c2 + (i64)c3 + (i64)c4;
}
